import java.io.*;
import java.util.*;

public class Main {

  public static void main(String[] args) throws Exception {
    Scanner scn = new Scanner(System.in);
    int n = scn.nextInt();
    int[] arr = new int[n];

    for(int i=0;i<n;i++){
      arr[i] = scn.nextInt();
    }

   

    Arrays.sort(arr);int x=1;
    System.out.print(arr[n-2] + " " + arr[x]);

  }
}
