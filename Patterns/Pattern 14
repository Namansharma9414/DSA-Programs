import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);

        int n = scn.nextInt();
        int c = 1;
         

        for(int i=1 ; i<=10 ; i++){
           System.out.println(n + " * " + c + " = " + (n*c) );
           c++;
        }

    }
}
