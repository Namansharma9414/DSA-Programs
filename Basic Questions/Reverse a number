import java.util.*;
class HelloWorld{
    
    static int reverse(int n){
        int rev = 0;
       while(n!=0){
           int rem = n%10;
           n = n/10;
           rev = rev*10 + rem;
       }
       return rev;
    }
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        
        int ans = reverse(n);
        System.out.println(ans);
        
    }   
}
